{
    "description": "Beta builds of Firefox: the popular open source web browser.",
    "homepage": "https://www.mozilla.org/en-US/firefox/beta/",
    "version": "78.0b5",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/78.0b5/win64/en-US/Firefox%20Setup%2078.0b5.exe#/dl.7z",
            "hash": "sha512:e584f48838f0feebb2eee6bf704e9d131c9aa5d61adaae0c39f6aa2a21ca1236fcaf0037ff1bcbadf06a4d58058d518cfecd7b0b0f7f1537a20ca029dec0344a"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/78.0b5/win32/en-US/Firefox%20Setup%2078.0b5.exe#/dl.7z",
            "hash": "sha512:c04da8a113a09f3eda47ba7cce34d4eac9e9fba974dac0be271801c22242daace8c91ad36518e6cd9b53689fe118256a234d1b0dd2e3e94107803be815fa7474"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-Profile \"$dir\\Profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-Profile Profile"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
