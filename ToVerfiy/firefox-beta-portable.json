{
    "description": "Beta builds of Firefox: the popular open source web browser.",
    "homepage": "https://www.mozilla.org/en-US/firefox/beta/",
    "version": "77.0b6",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/77.0b6/win64/en-US/Firefox%20Setup%2077.0b6.exe#/dl.7z",
            "hash": "sha512:5ab3eee2cfd5dd10efc17a7838ec6bf5eae8882308092b762644259d7fc8e84346d47c10f6e2ab89c29a6dd0a307229cd3ad35ee5a089edb3845c64afec68b41"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/77.0b6/win32/en-US/Firefox%20Setup%2077.0b6.exe#/dl.7z",
            "hash": "sha512:d662e4dac056605dd63794f3eb321c53b7b1491b26fc9364cd43b55dbb944790baf4ca5db1fb58cb381725a94a8522c7c79ae084338b35a2faf4abf073dff0e5"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-Profile \"$dir\\Profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-Profile Profile"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
