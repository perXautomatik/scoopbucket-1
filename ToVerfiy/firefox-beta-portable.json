{
    "description": "Beta builds of Firefox: the popular open source web browser.",
    "homepage": "https://www.mozilla.org/en-US/firefox/beta/",
    "version": "79.0b8",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/79.0b8/win64/en-US/Firefox%20Setup%2079.0b8.exe#/dl.7z",
            "hash": "9788137aa881e3db354bf8460ea63a5f6947761d9d1f2d750c5c202436396c76"
        },
        "32bit": {
            "url": "https://archive.mozilla.org/pub/firefox/releases/79.0b8/win32/en-US/Firefox%20Setup%2079.0b8.exe#/dl.7z",
            "hash": "f267c26e899f9bf9af2f15f0c7238abd41309d2f23c934342d1d3c2f081217d0"
        }
    },
    "extract_dir": "core",
    "bin": [
        [
            "firefox.exe",
            "firefox-beta",
            "-Profile \"$dir\\Profile\""
        ]
    ],
    "shortcuts": [
        [
            "firefox.exe",
            "Firefox Beta",
            "-Profile Profile"
        ]
    ],
    "checkver": {
        "url": "https://www.mozilla.org/de/firefox/all/",
        "regex": "<option value=\"desktop_beta\">([\\db.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win64/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win64/en-US/$basename"
                }
            },
            "32bit": {
                "url": "https://archive.mozilla.org/pub/firefox/releases/$version/win32/en-US/Firefox%20Setup%20$version.exe#/dl.7z",
                "hash": {
                    "url": "https://archive.mozilla.org/pub/firefox/releases/$version/SHA512SUMS",
                    "regex": "$sha512\\s+win32/en-US/$basename"
                }
            }
        }
    }
}
