{
  "$schema": "https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
  "homepage": "https://github.com/PerXautomatik/ScoopBucket",
  "description": "Setups my personal scoop bucket.",
  "notes": "Of course, this script is placed in the PerXautomatik bucket so you it presents a chicken and egg problem.",
  "version": "1.0.0",
  "installer": {
    "script": [
      "scoop bucket add PerXautomatik https://github.com/PerXautomatik/ScoopBucket.git"
    ]
  },
  "uninstaller": {
    "script": ["scoop bucket rm PerXautomatik"]
  },
  "url": "https://raw.githubusercontent.com/PerXautomatik/ScoopBucket/master/blank"
}
